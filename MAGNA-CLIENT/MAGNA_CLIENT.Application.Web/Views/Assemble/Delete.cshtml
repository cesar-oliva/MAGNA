@model MAGNA_CLIENT.Entities.Assemble
@{
    ViewData["Title"] = "Eliminar Empleado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container">

    <div class="row">
        <div class="col-sm-12">
            <h3>Delete Assembly</h3>
        </div>
    </div>
    <!--breadcrumb-->
 <nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="#">MAGNA</a></li>
    <li class="breadcrumb-item"><a href="#">Assemble</a></li>
    <li class="breadcrumb-item active" aria-current="page">Delete</li>
  </ol>
</nav>
    <div class="row">
        <div class="col-sm-10">
            <form method="post" asp-action="Delete">
                <input type="hidden" asp-for="Id"/>

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Id" class="control-label"></label>
                    <input asp-for="Id" class="form-control" disabled />
                    <span asp-validation-for="Id" class="text-danger"></span>
                </div>               
                <div class="form-group">
                    <label asp-for="AssembleCode" class="control-label"></label>
                    <input asp-for="AssembleCode" class="form-control" disabled />
                    <span asp-validation-for="AssembleCode" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="AssembleDescription" class="control-label"></label>
                    <input asp-for="AssembleDescription" class="form-control" disabled />
                    <span asp-validation-for="AssembleDescription" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Delete Assemble" class="btn-group-lg btn-primary" />
                </div>

            </form>
        </div>
    </div>
</div>

@section Scripts{
    @{
        <partial name="_ValidationScriptspartial" />
    }
}




